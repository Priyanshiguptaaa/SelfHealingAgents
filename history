cd .. && source .venv/bin/activate && cd backend && python -c "\
import asyncio\
from services.event_bus import event_bus\
from models.events import Event, EventType\
from datetime import datetime\
\
async def test():\
    try:\
        # Test event bus\
        await event_bus.connect()\
        \
        # Create test event\
        test_event = Event(\
            type=EventType.RETURN_API_FAILURE,\
            key='test-key',\
            payload={'test': 'data'},\
            ts=datetime.now(),\
            trace_id='test-123'\
        )\
        \
        # Publish event\
        await event_bus.publish(test_event)\
        print('Event published successfully')\
        \
        # Test subscription\
        async for event in event_bus.subscribe([EventType.RETURN_API_FAILURE]):\
            print('Event received:', event.type, event.trace_id)\
            break\
            \
        await event_bus.disconnect()\
        print('Event bus test completed')\
        \
    except Exception as e:\
        print('Event bus test error:', e)\
        import traceback\
        traceback.print_exc()\
\
asyncio.run(test())\
"
