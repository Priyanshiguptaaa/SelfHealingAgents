async def test_full_reasoning():\
    print('ðƒ¿§ƒ€ Testing Enhanced RCA Agent with Detailed Reasoning...')\
    \
    # Test RCA Agent\
    test_step = TraceStep(\
        trace_id='test-reasoning-123',\
        step='CheckReturnEligibility',\
        input={'sku': 'SKU-1001'},\
        output={},\
        schema_ok=False,\
        failure=FailureDetail(\
            type='SchemaMismatch',\
            field='return_policy',\
            message='return_policy field is missing'\
        ),\
        latency_ms=100,\
        timestamp=datetime.now()\
    )\
    \
    try:\
        # Test RCA analysis\
        rca_result = await rca_agent._analyze_with_anthropic(test_step)\
        print('\\nâƒ¼ƒ¥ RCA Analysis Result:')\
        print(f'Root Cause: {rca_result.get(\"root_cause\", \"N/A\")}')\
        print(f'Playbook: {rca_result.get(\"playbook\", \"N/A\")}')\
        print(f'Confidence: {rca_result.get(\"confidence\", \"N/A\")}')\
        \
        if 'reasoning' in rca_result:\
            print('\\nðƒ¿§ƒ€ DETAILED REASONING:')\
            reasoning = rca_result['reasoning']\
            for i, step in enumerate(reasoning.get('analysis_steps', []), 1):\
                print(f'{i}. {step}')\
            print(f'\\nEvidence: {reasoning.get(\"evidence\", \"N/A\")}')\
            print(f'Patterns: {reasoning.get(\"patterns_recognized\", \"N/A\")}')\
            print(f'Confidence Explanation: {reasoning.get(\"confidence_explanation\", \"N/A\")}')\
            \
        if 'recommendations' in rca_result:\
            print('\\nðƒ¿ƒ²ƒ RECOMMENDATIONS:')\
            recs = rca_result['recommendations']\
            print(f'Immediate Action: {recs.get(\"immediate_action\", \"N/A\")}')\
            print(f'Prevention: {recs.get(\"prevention\", \"N/A\")}')\
            print(f'Monitoring: {recs.get(\"monitoring\", \"N/A\")}')\
            \
    except Exception as e:\
        print(f'âƒ½ƒ¬ RCA Error: {e}')\
    \
    print('\\n' + '='*60)\
    print('ðƒ¿ƒ´§ Testing Enhanced Patch Generator with Reasoning...')\
    \
    try:\
        # Test Patch Generator\
        generator = PatchGenerator()\
        plan = type('MockPlan', (), {\
            'playbook': 'OutOfDateCatalogPolicy',\
            'cause': 'Missing return_policy field',\
            'risk_score': 0.3,\
            'confidence': 0.95,\
            'patch_spec': {\
                'type': 'add_field',\
                'file': 'catalog.py',\
                'change': '+ return_policy'\
            }\
        })()\
        \
        original_code = 'POLICY_FIELDS = [\"price\", \"inventory\", \"category\"]'\
        result = await generator.generate_patch(plan, original_code, 'test-reasoning-123')\
        \
        if result:\
            print('âƒ¼ƒ¥ Patch Generated with Reasoning!')\
            print(f'File: {result.file}')\
            print(f'LOC Changed: {result.loc_changed}')\
            print(f'Diff Lines: {len(result.diff_lines)}')\
            \
            # Show the diff\
            print('\\nðƒ¿ƒ³ƒ½ Code Changes:')\
            for line in result.diff_lines:\
                if line.startswith('+') or line.startswith('-'):\
                    print(line.rstrip())\
        else:\
            print('âƒ½ƒ¬ Patch generation failed')\
            \
    except Exception as e:\
        print(f'âƒ½ƒ¬ Patch Generator Error: {e}')\
